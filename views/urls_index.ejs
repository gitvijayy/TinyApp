<!DOCTYPE html>
<html lang="en">
<head>
  <% include ./partials/_header.ejs %>
</head>
<div class="container">
<body style="font-size:20px; padding:50px;">
    <!-- Add EJS code here -->
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">URL</th>
          <th scope="col">TinyURL</th>
          
          
        </tr>
      </thead>
      <tbody>
          <% var count = 0; %>
          <% for(var key in urls) {%>
        <tr>
            
          <th scope="row"><%= count += 1 %></th>
          <td><%=urls[key]%></td>
          <td><%=key%></td>
          
        
        </tr>
     <% }%>

     
     
      </tbody>
    </table>
    <!-- <a class="btn btn-primary btn-lg btn-block" href="#" role="button">Shorten new URL</a> -->
</body>
</div>
</html>